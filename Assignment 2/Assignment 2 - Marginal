class id timeclass;
model frequency = age chronicity time age*time group*time / dist=poisson ;
repeated subject=id / withinsubject=timeclass type=ind covb corrw modelse; run;

/*Model GEE-1a exch*/
proc genmod data=lda2.acu2 descending;
